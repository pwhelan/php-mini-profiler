<div id="pmp-profiler-details">
	<div>
		<b>{{ path }}</b>
		<hr/> 
		<div class="benchmark">
		{{#benchmarks}}
			<div style="float:left">{{name}}</div>
			<div style="float:right">{{time}} ms</div>
			<div style="clear:both"></div>
		{{/benchmarks}}
		</div>
		<hr/>
		<a class="showqueries">Show Queries</a>
		<div class="queries">
		{{#queries}}
			<h3><a href="#">{{summary}} {{time}}/ms</a></h3>
			<div>
				{{#backtrace}}
					<h4>{{class}}::{{function}}<h4/>
					{{#paths}}
						{{path}}<br/>
					{{/paths}}
				{{/backtrace}}
				<pre><code class="sql">{{query}}</code></pre>
			</div>
		{{/queries}}
		</div>
	</div>
	<br/>
</div>
